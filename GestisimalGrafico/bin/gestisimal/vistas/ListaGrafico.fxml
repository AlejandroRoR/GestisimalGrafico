<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="537.0" prefWidth="585.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gestisimal.vistas.ListaController">
   <children>
      <TableView fx:id="almaTable" prefHeight="496.0" prefWidth="585.0">
        <columns>
          <TableColumn fx:id="colCod" prefWidth="75.0" text="CÓDIGO" />
            <TableColumn fx:id="colDesc" prefWidth="173.0" text="DESCRIPCIÓN" />
          <TableColumn fx:id="colPCom" prefWidth="121.0" text="PRECIO COMPRA" />
            <TableColumn fx:id="colPVen" prefWidth="109.0" text="PRECIO VENTA" />
            <TableColumn fx:id="colNUni" prefWidth="104.0" text="Nº UNIDADES" />
        </columns>
      </TableView>
      <HBox alignment="CENTER" prefHeight="43.0" prefWidth="585.0">
         <children>
            <Button fx:id="viewBTN" mnemonicParsing="false" onAction="#cambiarVista" text="Cambiar Vista">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <HBox.margin>
                  <Insets left="100.0" right="100.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="modBTN" mnemonicParsing="false" onAction="#modArt" text="Modificar">
               <HBox.margin>
                  <Insets left="100.0" right="100.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
