<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="313.0" prefWidth="442.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gestisimal.vistas.ListaIndividualController">
   <children>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="379.0">
         <children>
            <Label text="CÓDIGO:">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="labCod">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="379.0">
         <children>
            <Label text="DESCRIPCIÓN:">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="labDesc">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="379.0">
         <children>
            <Label text="PRECIO COMPRA:">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="labPrecioCom">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="38.0" prefWidth="379.0">
         <children>
            <Label text="PRECIO VENTA:">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="labPrecioVen">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="379.0">
         <children>
            <Label text="Nº UNIDADES:">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="labNUni">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="65.0" prefWidth="442.0">
         <children>
            <Button mnemonicParsing="false" onAction="#disClave" text="&lt;">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Slider fx:id="slideCod" max="3.0" />
            <Button mnemonicParsing="false" onAction="#auClave" text="&gt;">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="51.0" prefWidth="379.0">
         <children>
            <Button fx:id="viewBTN" mnemonicParsing="false" onAction="#cambiarVista" text="Cambiar Vista">
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="modBTN" mnemonicParsing="false" onAction="#modArt" text="Modificar">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
